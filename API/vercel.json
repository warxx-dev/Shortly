{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/main.js"
    }
  ],
  "buildCommand": "npm run build",
  "devCommand": "npm run start:dev",
  "installCommand": "npm install",
  "framework": null,
  "outputDirectory": "dist"
}
